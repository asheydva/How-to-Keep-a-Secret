Cell[BoxData[
 RowBox[{
  RowBox[{"FailsCapitalizationTest", "[", "string_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "wordlist", "}"}], ",", 
    RowBox[{
     RowBox[{"wordlist", "=", 
      RowBox[{"StringSplit", "[", "string", "]"}]}], ";", 
     RowBox[{"MemberQ", "[", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"ToLowerCase", "[", 
             RowBox[{"StringDrop", "[", 
              RowBox[{"#1", ",", "1"}], "]"}], "]"}], "\[Equal]", 
            RowBox[{"StringDrop", "[", 
             RowBox[{"#1", ",", "1"}], "]"}]}], "||", 
           RowBox[{
            RowBox[{"ToUpperCase", "[", "#1", "]"}], "\[Equal]", "#1"}]}], 
          "&"}], ",", "wordlist"}], "]"}], ",", "False"}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
