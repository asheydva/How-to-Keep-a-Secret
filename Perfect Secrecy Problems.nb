Cell[BoxData[
 RowBox[{
  RowBox[{"RemoveBadStrings", "[", 
   RowBox[{"stringmatrix_", ",", "badstrings_"}], "]"}], ":=", 
  RowBox[{"Select", "[", 
   RowBox[{"stringmatrix", ",", 
    RowBox[{
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"stringlist", ",", "i", ",", "j"}], "}"}], ",", 
       RowBox[{
        RowBox[{"stringlist", "=", "#1"}], ";", 
        RowBox[{"Catch", "[", 
         RowBox[{
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"i", "=", "1"}], ",", 
            RowBox[{"i", "\[LessEqual]", 
             RowBox[{"Length", "[", "stringlist", "]"}]}], ",", 
            RowBox[{"i", "++"}], ",", 
            RowBox[{"For", "[", 
             RowBox[{
              RowBox[{"j", "=", "1"}], ",", 
              RowBox[{"j", "\[LessEqual]", 
               RowBox[{"Length", "[", "badstrings", "]"}]}], ",", 
              RowBox[{"j", "++"}], ",", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"StringContainsQ", "[", 
                 RowBox[{
                  RowBox[{"stringlist", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], ",", 
                  RowBox[{"badstrings", "[", 
                   RowBox[{"[", "j", "]"}], "]"}], ",", 
                  RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", 
                RowBox[{"Throw", "[", "False", "]"}]}], "]"}]}], "]"}]}], 
           "]"}], ";", 
          RowBox[{"Throw", "[", "True", "]"}]}], "]"}]}]}], "]"}], "&"}]}], 
   "]"}]}]], "Input",
