Cell[BoxData[
 RowBox[{
  RowBox[{"EncryptWithKey", "[", 
   RowBox[{"string_", ",", "secretkey_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "binary", "}"}], ",", 
    RowBox[{
     RowBox[{"binary", "=", 
      RowBox[{"StringToBinary", "[", 
