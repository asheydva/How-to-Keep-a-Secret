7cb13b65ae90"],

Cell[BoxData[
 RowBox[{
  RowBox[{"AddBinary", "[", 
   RowBox[{"binary1_", ",", "binary2_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bitlist1", ",", "bitlist2", ",", "bitlist3"}], "}"}], ",", 
    RowBox[{
     RowBox[{"bitlist1", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"ToExpression", ",", 
        RowBox[{"Characters", "[", "binary1", "]"}]}], "]"}]}], ";", 
     RowBox[{"bitlist2", "=", 
      RowBox[{"Map", "[", 
       RowBox[{"ToExpression", ",", 
        RowBox[{"Characters", "[", "binary2", "]"}]}], "]"}]}], ";", 
     RowBox[{"bitlist3", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"bitlist1", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "+", 
           RowBox[{"bitlist2", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ",", "2"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "bitlist1", "]"}]}], "}"}]}], "]"}]}], ";", 
     RowBox[{"StringJoin", "[", 
      RowBox[{"Map", "[", 
       RowBox[{"ToString", ",", "bitlist3"}], "]"}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10003.},
