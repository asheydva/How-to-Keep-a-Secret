Cell["\<\
This function takes a list of ciphertexts and tries to find all possible \
decryptions of the characters between the minimal index (\
\[OpenCurlyDoubleQuote]min\[CloseCurlyDoubleQuote]) and the maximal index (\
\[OpenCurlyDoubleQuote]max\[CloseCurlyDoubleQuote]). It automatically filters \
out any possibilities that have unlikely capitalization to be English text.\
\>", "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.775134307614998*^9, 3.775134373786318*^9}, {
   3.7751344555659165`*^9, 3.7751344714555144`*^9}, 
   3.775134535323635*^9},ExpressionUUID->"29aa93aa-b355-44a2-89f2-\
c86e9e64d04a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PossibleMultipleSeparations", "[", 
   RowBox[{"ciphertextlist_", ",", "min_", ",", "max_"}], "]"}], ":=", 
  RowBox[{"Select", "[", 
   RowBox[{
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "allmatchinglist", "}"}], ",", 
      RowBox[{
       RowBox[{"allmatchinglist", "=", 
        RowBox[{"Map", "[", 
         RowBox[{"Transpose", ",", 
          RowBox[{"Tuples", "[", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"PossibleSeparationsLong", "[", 
              RowBox[{"ciphertextlist", ",", "i"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "min", ",", "max"}], "}"}]}], "]"}], "]"}]}], 
         "]"}]}], ";", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Map", "[", 
           RowBox[{"StringJoin", ",", "#1"}], "]"}], "&"}], ",", 
         "allmatchinglist"}], "]"}]}]}], "]"}], ",", 
    RowBox[{
     RowBox[{"!", 
      RowBox[{"MemberQ", "[", 
       RowBox[{
        RowBox[{"Map", "[", 
         RowBox[{"FailsCapitalizationTest", ",", "#1"}], "]"}], ",", "True"}],
        "]"}]}], "&"}]}], "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
