Cell[CellGroupData[{

Cell["\<\
This function takes a list of possible decryptions, and sorts them based on \
how likely it is for them to be valid excerpts of English text. Since it does \
this using a search over all words in the Wolfram database, this is very slow \
and should only be used if the number of decryptions is small. On the other \
hand, the correct decryption is very likely to be in the top 5 results.\
\>", "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.7751417411218796`*^9, 3.7751418828709755`*^9}, 
   3.7751419741726665`*^9},ExpressionUUID->"5332bfed-4f6b-48f1-b9d1-\
8e9fe716e041"],

Cell[BoxData[
 RowBox[{
  RowBox[{"LikelihoodSort", "[", "stringmatrix_", "]"}], ":=", 
  RowBox[{"SortBy", "[", 
   RowBox[{"stringmatrix", ",", 
    RowBox[{
     RowBox[{"Total", "[", 
      RowBox[{"Map", "[", 
       RowBox[{"LikelihoodMeasure", ",", 
        RowBox[{"#1", "[", 
         RowBox[{"[", 
          RowBox[{"2", ";;"}], "]"}], "]"}]}], "]"}], "]"}], "&"}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
