Cell["\<\
This is an auxiliary function that measures how unlikely it is for the input \
to be an excerpt of English text. Since it has to search through the entire \
list of English words in Wolfram\[CloseCurlyQuote]s dictionary, it is \
somewhat slow.\
\>", "Text",
 CellChangeTimes->{{3.7751412860513897`*^9, 
  3.775141379133533*^9}},ExpressionUUID->"68ec1ba8-dc0f-42c5-9492-\
c981c1eb3657"],

Cell[BoxData[
 RowBox[{
  RowBox[{"LikelihoodMeasure", "[", "string_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"stringlist", ",", "measure", ",", "i"}], "}"}], ",", 
    RowBox[{
     RowBox[{"stringlist", "=", 
      RowBox[{"StringSplit", "[", "string", "]"}]}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"DictionaryLookup", "[", 
         RowBox[{
          RowBox[{"__", "~~", 
           RowBox[{"stringlist", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], ",", "1"}], "]"}], "\[Equal]", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"measure", "=", "1"}], ",", 
       RowBox[{"measure", "=", "0"}]}], "]"}], ";", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "2"}], ",", 
       RowBox[{"i", "<", 
        RowBox[{"Length", "[", "stringlist", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"DictionaryWordQ", "[", 
           RowBox[{"stringlist", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}]}], ",", 
         RowBox[{"measure", "=", 
          RowBox[{"measure", "+", "1"}]}]}], "]"}]}], "]"}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"StringTake", "[", 
         RowBox[{"string", ",", 
          RowBox[{"-", "1"}]}], "]"}], "\[Equal]", "\"\< \>\""}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"DictionaryWordQ", "[", 
           RowBox[{"stringlist", "[", 
            RowBox[{"[", 
             RowBox[{"-", "1"}], "]"}], "]"}], "]"}]}], ",", 
         RowBox[{"measure", "=", 
          RowBox[{"measure", "+", "1"}]}]}], "]"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"DictionaryLookup", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"stringlist", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "~~", "__"}], ",", "1"}], "]"}],
           "\[Equal]", 
          RowBox[{"{", "}"}]}], ",", 
         RowBox[{"measure", "=", 
          RowBox[{"measure", "+", "1"}]}]}], "]"}]}], "]"}], ";", 
     "measure"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.7749624425800285`*^9, 3.774962643524367*^9}, {
  3.774962723917898*^9, 3.774962851830901*^9}, {3.774962883239992*^9, 
