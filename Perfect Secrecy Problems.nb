Cell[CellGroupData[{

Cell["\<\
This function takes a sequence of characters (the \"string\") and converts \
into a binary sequence using standard ASCII encoding. The string is padded \
with 0 s at the end so that it is a fixed length (\"len\") long.\
\>", "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.775057819861472*^9, 3.775057908199252*^9}, 
   3.775057983688001*^9, 3.7751316182635293`*^9, 
   3.775131725044446*^9},ExpressionUUID->"4570ba69-e7cb-471a-9a8e-\
c47ac695200e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"StringToBinary", "[", 
   RowBox[{"string_", ",", "len_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"integerlist", ",", "bitlist", ",", "basestring"}], "}"}], ",", 
    RowBox[{
     RowBox[{"integerlist", "=", 
      RowBox[{"ToCharacterCode", "[", "string", "]"}]}], ";", 
     RowBox[{"bitlist", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"IntegerDigits", "[", 
           RowBox[{"#1", ",", "2", ",", "8"}], "]"}], "&"}], ",", 
         "integerlist"}], "]"}], "]"}]}], ";", 
     RowBox[{"basestring", "=", 
      RowBox[{"StringJoin", "[", 
       RowBox[{"Map", "[", 
        RowBox[{"ToString", ",", "bitlist"}], "]"}], "]"}]}], ";", 
     RowBox[{"StringPadRight", "[", 
      RowBox[{"basestring", ",", "len", ",", "\"\<0\>\""}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
