Cell[BoxData[
 RowBox[{
  RowBox[{"BinaryToStringClean", "[", "binary_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"binarylist", ",", "integerlist"}], "}"}], ",", 
    RowBox[{
     RowBox[{"binarylist", "=", 
      RowBox[{"StringPartition", "[", 
       RowBox[{"binary", ",", "8"}], "]"}]}], ";", 
     RowBox[{"binarylist", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"binarylist", ",", 
        RowBox[{
         RowBox[{"#1", "\[NotEqual]", "\"\<00000000\>\""}], "&"}]}], "]"}]}], 
     ";", 
     RowBox[{"integerlist", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"FromDigits", "[", 
          RowBox[{
           RowBox[{"Map", "[", 
            RowBox[{"ToExpression", ",", 
             RowBox[{"StringPartition", "[", 
              RowBox[{"#1", ",", "1"}], "]"}]}], "]"}], ",", "2"}], "]"}], 
         "&"}], ",", "binarylist"}], "]"}]}], ";", 
     RowBox[{"FromCharacterCode", "[", "integerlist", "]"}]}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10002.},
