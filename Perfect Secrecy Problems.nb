Cell[CellGroupData[{

Cell["\<\
This function is similar to PossibleSeparations, except that it accepts a \
list of ciphertexts rather than just a pair of them, and it returns the key \
that would correspond to each possible decryption.\
\>", "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.7751335300711007`*^9, 3.775133582190713*^9}, 
   3.775133892699723*^9},ExpressionUUID->"3a035d2c-12ab-4915-9a57-\
02fe6ebc547c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PossibleSeparationsLong", "[", 
   RowBox[{"ciphertextlist_", ",", "index_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "encodedlist", ",", "possibleSeparationList", ",", "decoding", ",", "i", 
      ",", "bin", ",", "key", ",", "validpossibility", ",", "j"}], "}"}], ",", 
    RowBox[{
     RowBox[{"encodedlist", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"StringTake", "[", 
          RowBox[{"#1", ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"1", "+", 
              RowBox[{"8", "*", 
               RowBox[{"(", 
                RowBox[{"index", "-", "1"}], ")"}]}]}], ",", 
             RowBox[{"8", "*", "index"}]}], "}"}]}], "]"}], "&"}], ",", 
        "ciphertextlist"}], "]"}]}], ";", 
     RowBox[{"possibleSeparationList", "=", 
      RowBox[{"{", "}"}]}], ";", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "[", "validcharacters", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{
        RowBox[{"bin", "=", 
         RowBox[{"StringToBinary", "[", 
          RowBox[{
           RowBox[{"validcharacters", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", "8"}], "]"}]}], ";", 
        RowBox[{"key", "=", 
         RowBox[{"AddBinary", "[", 
          RowBox[{"bin", ",", 
           RowBox[{"encodedlist", "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
        RowBox[{"decoding", "=", 
         RowBox[{"{", 
          RowBox[{"key", ",", 
           RowBox[{"validcharacters", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], ";", 
        RowBox[{"validpossibility", "=", "True"}], ";", 
        RowBox[{"j", "=", "2"}], ";", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"validpossibility", "&&", 
           RowBox[{"j", "\[LessEqual]", 
            RowBox[{"Length", "[", "encodedlist", "]"}]}]}], ",", 
          RowBox[{
           RowBox[{"bin", "=", 
            RowBox[{"AddBinary", "[", 
             RowBox[{
              RowBox[{"encodedlist", "[", 
               RowBox[{"[", "j", "]"}], "]"}], ",", "key"}], "]"}]}], ";", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"MemberQ", "[", 
              RowBox[{"validbinary", ",", "bin"}], "]"}], ",", 
             RowBox[{"decoding", "=", 
              RowBox[{"Append", "[", 
               RowBox[{"decoding", ",", 
                RowBox[{"BinaryToString", "[", "bin", "]"}]}], "]"}]}], ",", 
             RowBox[{"validpossibility", "=", "False"}]}], "]"}], ";", 
           RowBox[{"j", "++"}]}]}], "]"}], ";", 
        RowBox[{"If", "[", 
         RowBox[{"validpossibility", ",", 
          RowBox[{"possibleSeparationList", "=", 
           RowBox[{"Append", "[", 
            RowBox[{"possibleSeparationList", ",", "decoding"}], "]"}]}]}], 
         "]"}]}]}], "]"}], ";", "possibleSeparationList"}]}], 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.7747236716972103`*^9, 3.7747237298982973`*^9}, {
